call SetUnitVertexColor(u2, 255, 0, 0, 255)
call SaveUnitHandle(udg_hash, Id, 1, u)
call SaveUnitHandle(udg_hash, Id, 2, u2)
call SaveReal(udg_hash, Id, 1, dmg)
call SaveReal(udg_hash, Id, 2, aoe)
