local unit dami
local real x1
local real y1
if time>0 and GetUnitHealth(target)>0.00 then
call SaveReal(udg_Hash,id,StringHash("Inferno|DmgPeriodic"),dmgPeriodic+periodic)
loop
exitwhen i>wisps
set light=LoadLightningHandle(udg_Hash,id,StringHash("Inferno|Light"+I2S(i)))
