                if heroHasItemById(target , ingId) then
                    call removeHeroItemById(target , ingId)
                    set libBuy___ITEMS[index]=ingId
                else
                    set libBuy___ITEMS[index]=0
                    call libBuy___returnHeroItems(target)
                    call libBuy___clearCollectedItems()
                    return false
                endif
            endif
            
            set index=index + 1
        endloop
        
        call libBuy___clearCollectedItems()
        
        return true
