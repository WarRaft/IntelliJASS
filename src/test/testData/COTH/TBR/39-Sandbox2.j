function Trig_Sandbox_Func001002 takes nothing returns nothing
    call SetPlayerStateBJ(GetEnumPlayer(),PLAYER_STATE_RESOURCE_GOLD,500000)
    endfunction
    function Trig_Sandbox_Func002002 takes nothing returns nothing
    call SetPlayerStateBJ(GetEnumPlayer(),PLAYER_STATE_RESOURCE_LUMBER,450000)
    endfunction
    function Trig_Sandbox_Actions takes nothing returns nothing
    call ForForce(GetPlayersAll(),function Trig_Sandbox_Func001002)
    call ForForce(GetPlayersAll(),function Trig_Sandbox_Func002002)
    call AdjustPlayerStateBJ(50000,Player(0),PLAYER_STATE_RESOURCE_LUMBER)
    call AdjustPlayerStateBJ(50000,Player(1),PLAYER_STATE_RESOURCE_LUMBER)
    call AdjustPlayerStateBJ(50000,Player(2),PLAYER_STATE_RESOURCE_LUMBER)
    call AdjustPlayerStateBJ(50000,Player(3),PLAYER_STATE_RESOURCE_LUMBER)
    call AdjustPlayerStateBJ(50000,Player(4),PLAYER_STATE_RESOURCE_LUMBER)
    call AdjustPlayerStateBJ(50000,Player(5),PLAYER_STATE_RESOURCE_LUMBER)
    call AdjustPlayerStateBJ(50000,Player(6),PLAYER_STATE_RESOURCE_LUMBER)
    call AdjustPlayerStateBJ(50000,Player(7),PLAYER_STATE_RESOURCE_LUMBER)
    call AdjustPlayerStateBJ(50000,Player(8),PLAYER_STATE_RESOURCE_LUMBER)
    call AdjustPlayerStateBJ(50000,Player(9),PLAYER_STATE_RESOURCE_LUMBER)
    call AdjustPlayerStateBJ(50000,Player(10),PLAYER_STATE_RESOURCE_LUMBER)
    call AdjustPlayerStateBJ(50000,Player(11),PLAYER_STATE_RESOURCE_LUMBER)
    call DisableTrigger(gg_trg_Team_Killing_Alliance)
    call DisableTrigger(gg_trg_Team_Killing_Horde)
    call DisableTrigger(gg_trg_Throne_Reduce)
    call DisableTrigger(gg_trg_Allied_Victory_Two)
    call DisableTrigger(gg_trg_Horde_Victory)
    call DisableTrigger(gg_trg_Horde_Victory)
    call EnableTrigger(gg_trg_Sandbox_Creeps)
    call EnableTrigger(gg_trg_Sandbox_Refresh)
    call EnableTrigger(gg_trg_Item_Orb_On)
    call SetForceAllianceStateBJ(GetPlayersAll(),GetPlayersAll(),bj_ALLIANCE_ALLIED_ADVUNITS)
    call SetForceAllianceStateBJ(GetPlayersAll(),bj_FORCE_PLAYER[PLAYER_NEUTRAL_AGGRESSIVE],bj_ALLIANCE_UNALLIED_VISION)
    call SetForceAllianceStateBJ(bj_FORCE_PLAYER[PLAYER_NEUTRAL_AGGRESSIVE],GetPlayersAll(),bj_ALLIANCE_UNALLIED_VISION)
    call TriggerRegisterPlayerUnitEventSimple(gg_trg_Trophies_and_Libation_and_Teron_Reset,Player(PLAYER_NEUTRAL_AGGRESSIVE),EVENT_PLAYER_UNIT_DEATH)
    set udg_TempPoint=GetRandomLocInRect(gg_rct_Sandbox)
    call CreateNUnitsAtLoc(1,'nogm',Player(PLAYER_NEUTRAL_AGGRESSIVE),udg_TempPoint,bj_UNIT_FACING)
    call RemoveLocation(udg_TempPoint)
    set udg_TempPoint=GetRandomLocInRect(gg_rct_Sandbox)
    call CreateNUnitsAtLoc(1,'nogm',Player(PLAYER_NEUTRAL_AGGRESSIVE),udg_TempPoint,bj_UNIT_FACING)
    call RemoveLocation(udg_TempPoint)
    set udg_TempPoint=GetRandomLocInRect(gg_rct_Sandbox)
    call CreateNUnitsAtLoc(1,'nogm',Player(PLAYER_NEUTRAL_AGGRESSIVE),udg_TempPoint,bj_UNIT_FACING)
    call RemoveLocation(udg_TempPoint)
    set udg_TempPoint=GetRandomLocInRect(gg_rct_Sandbox)
    call CreateNUnitsAtLoc(1,'nogl',Player(PLAYER_NEUTRAL_AGGRESSIVE),udg_TempPoint,bj_UNIT_FACING)
    call RemoveLocation(udg_TempPoint)
    set udg_TempPoint=GetRandomLocInRect(gg_rct_Sandbox)
    call CreateNUnitsAtLoc(1,'Ekgg',Player(PLAYER_NEUTRAL_AGGRESSIVE),udg_TempPoint,bj_UNIT_FACING)
    call RemoveLocation(udg_TempPoint)
    call DisplayTimedTextToForce(GetPlayersAll(),40.00,"|nYou've been granted 500k gold and IP, victory conditions have been disabled, friendly fire has been enabled, and a creep camp will spawn and respawn below Stormgarde automatically.  A Tome of Experience can be bought at the bookshelf to level your hero rapidly. Type -refresh to reset the cooldowns on abilities, however using refresh may cause certain abilities to bug if the hero they belong to was not intended to use a Journey's End.|n|nType \"iseedeadpeople\" to disable fog of war.|n|nEnjoy!")
    call AddItemToStockBJ('texp',gg_unit_ngwr_0458,1,1)
    endfunction
    function InitTrig_Sandbox2 takes nothing returns nothing
    set gg_trg_Sandbox2=CreateTrigger()
    call TriggerRegisterVariableEvent(gg_trg_Sandbox2,"udg_Sandbox",EQUAL,11.00)
    call TriggerAddAction(gg_trg_Sandbox2,function Trig_Sandbox_Actions)
    endfunction