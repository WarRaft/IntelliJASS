function Trig_Split_Func002C takes nothing returns boolean
    if ( not ( udg_B >= 852002 ) ) then
        return false
    endif
    if ( not ( udg_B <= 852007 ) ) then
        return false
    endif
    if ( not ( GetOrderTargetItem() == UnitItemInSlotBJ(GetOrderedUnit(), ( udg_B - 852001 )) ) ) then
        return false
    endif
    if ( not ( GetItemCharges(GetOrderTargetItem()) > 1 ) ) then
        return false
    endif
    return true
endfunction

function Trig_Split_Actions takes nothing returns nothing
    set udg_B = GetIssuedOrderId()
    if ( Trig_Split_Func002C() ) then
        call SetItemCharges( GetOrderTargetItem(), ( GetItemCharges(GetOrderTargetItem()) - 1 ) )
        set udg_A = GetUnitLoc(GetOrderedUnit())
        call CreateItemLoc( GetItemTypeId(GetOrderTargetItem()), udg_A )
        call RemoveLocation(udg_A)
        call SetItemCharges( GetLastCreatedItem(), 1 )
    else
    endif
endfunction

