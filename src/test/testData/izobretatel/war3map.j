function Trig_StartGold_Actions takes nothing returns nothing
    local integer Gold = 0
    local integer i = - 1
    loop
        set i = i + 1
        exitwhen i > 11
        if GetPlayerController(Player(i)) == MAP_CONTROL_NONE then
            set Gold = Gold + 1 
        else
            if GetPlayerSlotState(Player(i)) == PLAYER_SLOT_STATE_LEFT then
                set Gold = Gold + 1
            else
                if GetPlayerSlotState(Player(i)) == PLAYER_SLOT_STATE_EMPTY then
                    set Gold = Gold + 1
                endif
            endif
        endif
    endloop
    set i = - 1
    loop
        set i = i + 1
        exitwhen i > 11
        call AdjustPlayerStateBJ(Gold * 15 , Player(i), PLAYER_STATE_RESOURCE_GOLD )
    endloop
    call DisplayTimedTextToForce( GetPlayersAll(), 30, "|c00FFFF00Стартовое золото!|r" )
endfunction

