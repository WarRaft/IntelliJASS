// Trigger: WaveLight
//===========================================================================
//TESH.scrollpos=0
//TESH.alwaysfold=0
function Vistrel1 takes nothing returns nothing
    local timer timerAct = GetExpiredTimer()
    local integer id = GetHandleId(timerAct)
    local unit caster = LoadUnitHandle(udg_Hash, id, 1)
    local unit dummy = LoadUnitHandle(udg_Hash, id, 2)
    local real process = LoadReal(udg_Hash, id, 8) + LoadReal(udg_Hash, id, 7)
    local group groupAct = CreateGroup()
    local group groupP = CreateGroup()
    local unit u = null
    local real speed = LoadReal(udg_Hash, id, 7)
        
    if process > LoadReal(udg_Hash, id, 4) then
        call RemoveUnit(dummy)
        call FlushChildHashtable(udg_Hash, id)
        call PauseTimer(timerAct)
        call DestroyTimer(timerAct)
    else
        call SaveReal(udg_Hash, id, 8, process)
        call SetUnitX(dummy, GetUnitX(dummy) + speed * LoadReal(udg_Hash, id, 9))
        call SetUnitY(dummy, GetUnitY(dummy) + speed * LoadReal(udg_Hash, id, 0))
        call GroupEnumUnitsInRange(groupAct, GetUnitX(dummy), GetUnitY(dummy), LoadReal(udg_Hash, id,6), null)
        loop
            set u = FirstOfGroup(groupAct)
            exitwhen u == null
            if IsUnitAlly(u, GetOwningPlayer(caster)) or GetUnitState(u, UNIT_STATE_LIFE) <= 0 then
                call GroupRemoveUnit(groupAct,u)
            else
                call UnitDamageTarget(caster, u, LoadReal(udg_Hash, id, 3), false, false, ATTACK_TYPE_NORMAL, DAMAGE_TYPE_NORMAL, null)
                call RemoveUnit(dummy)
                call FlushChildHashtable(udg_Hash, id)
                call PauseTimer(timerAct)
                call DestroyTimer(timerAct)
            endif
        endloop
        call DestroyGroup(groupAct)
    endif
    
    set caster = null
    set dummy = null
    set timerAct = null
    set groupAct = null
    set groupP = null
endfunction

function Trig_WaveLight_Actions takes nothing returns nothing
    local unit caster = GetTriggerUnit()
    local real xCaster = GetUnitX(caster)
    local real yCaster = GetUnitY(caster)
    local real xTarget = GetSpellTargetX()
    local real yTarget = GetSpellTargetY()
    local real angle = Atan2(yTarget - yCaster, xTarget - xCaster)


    local unit dummy = CreateUnit(GetOwningPlayer(caster), 'h02Q', xCaster, yCaster, angle)
    
    local real maxDist = 800
    local real damage = GetHeroAgi(caster,true)
    local real aoe = 70
    local real speed = 1000

    local timer timerAct = CreateTimer()
    local integer id = GetHandleId(timerAct)
    if xCaster == xTarget and yCaster == yTarget then
        set angle = GetUnitFacing(caster) * bj_DEGTORAD
    endif
    call SetUnitX(dummy, xCaster + aoe * 1.33 * Cos(angle))
    call SetUnitY(dummy, yCaster + aoe * 1.33 * Sin(angle))
    call SetUnitFacing(dummy, GetUnitFacing(caster))
    
    call SaveUnitHandle(udg_Hash, id,1, caster)
    call SaveUnitHandle(udg_Hash, id, 2, dummy)
    
    call SaveReal(udg_Hash, id, 3, damage)
    call SaveReal(udg_Hash, id,4, maxDist)
    call SaveReal(udg_Hash, id, 5, angle)
    call SaveReal(udg_Hash, id,6, aoe)
    call SaveReal(udg_Hash, id, 7, speed  * 0.03)
    call SaveReal(udg_Hash, id,8, 0)
    call SaveReal(udg_Hash, id,9, Cos(angle))
    call SaveReal(udg_Hash, id,0, Sin(angle))
    
    call TimerStart(timerAct, 0.03, true, function Vistrel1)
    
    set caster = null
    set dummy = null
    set timerAct = null
endfunction

