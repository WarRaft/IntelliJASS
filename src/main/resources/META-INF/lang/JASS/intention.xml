<!-- https://plugins.jetbrains.com/docs/intellij/code-intentions.html#running-the-plugin -->

<idea-plugin>
    <extensions defaultExtensionNs="com.intellij">

        <intentionAction>
            <language>JASS</language>
            <className>raft.war.language.jass.intention.number.converter.JassIntentionIntToRawCode</className>
            <category>JASS/Number</category>
        </intentionAction>

        <intentionAction>
            <language>JASS</language>
            <className>raft.war.language.jass.intention.number.converter.JassIntentionIntToHex</className>
            <category>JASS/Number</category>
        </intentionAction>

        <intentionAction>
            <language>JASS</language>
            <className>raft.war.language.jass.intention.number.converter.JassIntentionHexToInt</className>
            <category>JASS/Number</category>
        </intentionAction>

        <intentionAction>
            <language>JASS</language>
            <className>raft.war.language.jass.intention.number.converter.JassIntentionHexToRawCode</className>
            <category>JASS/Number</category>
        </intentionAction>

        <intentionAction>
            <language>JASS</language>
            <className>raft.war.language.jass.intention.number.converter.JassIntentionRawCodeToInt</className>
            <category>JASS/Number</category>
        </intentionAction>

        <intentionAction>
            <language>JASS</language>
            <className>raft.war.language.jass.intention.number.converter.JassIntentionRawCodeToHex</className>
            <category>JASS/Number</category>
        </intentionAction>

    </extensions>
</idea-plugin>
